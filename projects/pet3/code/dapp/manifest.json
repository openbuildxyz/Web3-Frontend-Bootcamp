{
  "manifest_version": 3,
  "name": "Pet3",
  "description": "A virtual pet from Web3",
  "version": "0.1",
  "action": {
    "default_popup": "app.html"
  },
  "icons": {
    "16": "img/pet3.png",
    "32": "img/pet3.png",
    "48": "img/pet3.png",
    "128": "img/pet3.png"
  },
  "content_scripts": [
    {
      "js": ["src/main.tsx"],
      "matches": ["<all_urls>"]
    }
  ],
  "permissions": ["storage"]
}
